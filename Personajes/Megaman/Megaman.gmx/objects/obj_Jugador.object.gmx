            <kind>1</kind>
            <string>///Movimiento y Salto 


//Derecha: Mirar el script movimiento, el valor entre parentesis afecta al argument0
if keyboard_check(global.Derecha) &amp;&amp; ! keyboard_check(global.Izquierda)   {
 Scr_Movimiento(1) 
  image_speed=0.4; 
  //image_index=1 
}


//Deteniendo Animacion
if keyboard_check_released(global.Derecha){
    image_speed=0;
    image_index=0
}   


//Izquierda: Mirar el script movimiento, el valor entre parentesis afecta al argument0
if keyboard_check(global.Izquierda)&amp;&amp; ! keyboard_check(global.Derecha) {
Scr_Movimiento(-1)
  image_speed=0.4; 
  //image_index=1 
}

//Deteniendo Animacion
if keyboard_check_released(global.Izquierda){
    image_speed=0;
    image_index=0;
    }


//Ataque    
    if keyboard_check_pressed(global.Ataque)
   {
        if Sayan=0
        { 
         sprite_index=spr_megaman_atacando;
         image_speed=0.5;
        }
        else
        {
         sprite_index=spr_supermegaman_atacando;
         image_speed=0.5;
        }
   }
//Deteniendo animación de ataque  
   if keyboard_check_released(global.Ataque)
   {
        if Sayan=0
        {
         sprite_index=spr_megaman_caminando;
         image_index=0;
         image_speed=0;
        }
        else
        {
         sprite_index=spr_supermegaman_caminando;
         image_index=0;
         image_speed=0;
        }
   }
   
//Salto
if keyboard_check_pressed(global.Salto) &amp;&amp; !place_free(x,y+1)
   {
        if Sayan=0
        {
         vspeed=-12;
         
         sprite_index=spr_megaman_saltando

        }
        else
        {
          vspeed=-12;
          
         sprite_index=spr_supermegaman_saltando
    
        }
    }
    
//Proyectil
if keyboard_check_pressed(global.Disparar) 
   {
        if Sayan=0
        {
         sprite_index=spr_megaman_distancia
         image_speed=0
         image_index=2
         
            if image_xscale=1
            {
            kame=instance_create(x+30,y-42,obj_shot_right);
            kame.direction=0;
            kame.speed=15;
            }
            else if image_xscale=-1
            {
            kame=instance_create(x-30,y-42,obj_shot_left);
            kame.direction=180;
            kame.speed=15;
            }
         
        }
        else
        {
         sprite_index=spr_supermegaman_distancia
         image_speed=0
         image_index=2
         
            if image_xscale=1
            {
            kame=instance_create(x+30,y,obj_shot_right);
            kame.direction=0;
            kame.speed=15;
            }
            else if image_xscale=-1
            {
            kame=instance_create(x-30,y,obj_shot_left);
            kame.direction=180;
            kame.speed=15;
            }
        }
    }  

    //Deteniendo animación de ataque  
   if keyboard_check_released(global.Disparar)
   {
        if Sayan=0
        {
         sprite_index=spr_megaman_caminando;
         image_index=0;
         image_speed=0;
         hspeed=0;
        }
        else
        {
         sprite_index=spr_supermegaman_caminando;
         image_index=0;
         image_speed=0;
         hspeed=0;
        }
   }
   
   
   
//Especial    
    if keyboard_check(global.Especial)
   {
       
        Carga += 1;

         sprite_index=spr_megaman_cargando;
         image_speed=0.8;
        
        
   }
//Deteniendo animación de ataque  
   if keyboard_check_released(global.Especial)
   {
   
        sprite_index=spr_megaman_especial;
        if Carga &lt; room_speed * 2{
        
            if image_xscale=1
            {
            kame=instance_create(x+30,y-42,obj_shot_right);
            kame.direction=0;
            kame.speed=15;
            }
            else if image_xscale=-1
            {
            kame=instance_create(x-30,y-42,obj_shot_left);
            kame.direction=180;
            kame.speed=15;
            }
            
        }
        if Carga &gt;= room_speed * 2 &amp;&amp; Carga &lt; room_speed * 3{
        
            if image_xscale=1
            {
            kame2=instance_create(x+30,y-42,obj_shot_power1_right);
            kame2.direction=0;
            kame2.speed=15;
            }
            else if image_xscale=-1
            {
            kame2=instance_create(x-30,y-42,obj_shot_power1_left);
            kame2.direction=180;
            kame2.speed=15;
            }
        
        }
        
        if Carga &gt;= room_speed * 3{
        
            if image_xscale=1
            {
            kame3=instance_create(x+30,y-42,obj_shot_power2_right);
            kame3.direction=0;
            kame3.speed=15;
            }
            else if image_xscale=-1
            {
            kame3=instance_create(x-30,y-42,obj_shot_power2_left);
            kame3.direction=180;
            kame3.speed=15;
            }
        
        }
        
        Carga = 0
         //sprite_index=spr_megaman_caminando;
         //image_index=0;
        // image_speed=0;
        
        
   }
    
   
    
    
    
    
    
