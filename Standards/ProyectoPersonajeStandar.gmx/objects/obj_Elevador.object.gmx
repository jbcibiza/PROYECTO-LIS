/*==========================================*/
      //Si esta moviendose a la derecha
/*==========================================*/

if Dir="Horizontal"{
    if !place_free(x+2,y){Dir="-Horizontal";exit}
    if place_meeting(x+2,y,obj_PNoSolid){Dir="-Horizontal";exit}
    x+=2
    
    
    //Colicion con el jugador
    var Jugador;
    
    Jugador=collision_rectangle(x,y,x+64,y-16,obj_Jugador,0,0)
    
    //Si esta en Colicion con el Jugador
    if (Jugador){
        
    
    //Evita conflictos si  Dir= "Vertical" o "-Vertical"
        if Jugador.gravity=0{
           Jugador.x+=2 
        }
    }
}


/*==========================================*/
      //Si esta moviendose a la izquierda
/*==========================================*/
 
 if Dir="-Horizontal"{
     if !place_free(x-2,y){Dir="Horizontal";exit}
     if place_meeting(x-2,y,obj_PNoSolid){Dir="Horizontal";exit}
    x-=2
 //Colicion con el jugador
 var Jugador;
    
    
    Jugador=collision_rectangle(x,y,x+64,y-16,obj_Jugador,0,0)
    
    //Si esta en Colicion con el Jugador
    if (Jugador){
        
    //Evita conflictos si  Dir= "Vertical" o "-Vertical"
        if Jugador.gravity=0{
           Jugador.x-=2 
        }
    }

}
