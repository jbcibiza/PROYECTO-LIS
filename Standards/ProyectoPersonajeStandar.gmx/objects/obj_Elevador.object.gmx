if Dir="-Vertical"{

//Colicion con el jugador 
      var Jugador;    
    
    Jugador=collision_rectangle(x,y,x+64,y-16,obj_Jugador,0,0)
    
    //Verifica si esta el jugador encima para cambiar el chekeo delas coliciones con los objetos solidos 
    if (Jugador){ 
        if !place_free(x,y-36){
            Dir="Vertical";exit}
            }
            
            else{
     // si no esta el jugador encima, Chekea la Coliciona normalmente  
    if !place_free(x,y-2){Dir="Vertical";exit}
    }
    y-=2
    
    //Si esta en Colicion con el Jugador
    if (Jugador){
        
    //Evita conflictos si  Dir= "Vertical" o "-Vertical"
        if Jugador.gravity=0{
           Jugador.y-=2 
        }
    }
}


/*==========================================*/
      //Si esta moviendose hacia abajo
/*==========================================*/
 
 if Dir="Vertical"{
  
     
      
         if !place_free(x,y+2){Dir="-Vertical";exit}  
         if place_meeting(x,y+2,obj_PNoSolid){Dir="-Vertical";exit}
         y+=2
    
         var Jugador;    
        //Colicion con el jugador
        Jugador=collision_rectangle(x,y,x+64,y-16,obj_Jugador,0,0)
        //Si esta en Colicion con el Jugador
        if (Jugador){
            
        //Evita conflictos si  Dir= "Vertical" o "-Vertical"
            if Jugador.gravity=0{
               Jugador.y+=2 
            }
        }
    }

