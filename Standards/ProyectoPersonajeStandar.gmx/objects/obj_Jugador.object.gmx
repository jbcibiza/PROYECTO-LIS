//Derecha: Mirar el script movimiento, el valor entre parentesis afecta al argument0
if keyboard_check(global.Derecha) &amp;&amp; ! keyboard_check(global.Izquierda)   {
 Scr_Movimiento(1) 
  image_speed=0; 
  image_index=1 
}


//Deteniendo Animacion
if keyboard_check_released(global.Derecha){
    image_speed=0;
    image_index=0
}   


//Izquierda: Mirar el script movimiento, el valor entre parentesis afecta al argument0
if keyboard_check(global.Izquierda)&amp;&amp; ! keyboard_check(global.Derecha) {
Scr_Movimiento(-1)
  image_speed=0; 
  image_index=1 
}

//Deteniendo Animacion
if keyboard_check_released(global.Izquierda){
    image_speed=0;
    image_index=0;
    }


//Ataque    
    if keyboard_check_pressed(global.Ataque)
   {
        if Sayan=0
        { 
         sprite_index=GokuOstia;
         image_speed=0.5;
        }
        else
        {
         sprite_index=SayanOstia;
         image_speed=0.5;
        }
   }
//Deteniendo animación de ataque  
   if keyboard_check_released(global.Ataque)
   {
        if Sayan=0
        {
         sprite_index=spr_Caminando;
         image_index=0;
         image_speed=0;
        }
        else
        {
         sprite_index=sprSayan_Caminando;
         image_index=0;
         image_speed=0;
        }
   }
   
//Salto
if keyboard_check_pressed(global.Salto) &amp;&amp; !place_free(x,y+1)
   {
        if Sayan=0
        {
         vspeed=-12;
         sprite_index=spr_Salto
        }
        else
        {
          vspeed=-12;
         sprite_index=sprSayan_Salto
        }
    }
    
//Proyectil
if keyboard_check_pressed(global.Disparar) 
   {
        if Sayan=0
        {
         sprite_index=spr_GokuKame
         image_speed=0
         image_index=2
         
            if image_xscale=1
            {
            kame=instance_create(x,y,obj_Kame);
            kame.direction=0;
            kame.speed=15;
            }
            else if image_xscale=-1
            {
            kame=instance_create(x,y,obj_KameLeft);
            kame.direction=180;
            kame.speed=15;
            }
         
        }
        else
        {
         sprite_index=spr_SayanKame
         image_speed=0
         image_index=2
         
            if image_xscale=1
            {
            kame=instance_create(x,y,obj_Kame);
            kame.direction=0;
            kame.speed=15;
            }
            else if image_xscale=-1
            {
            kame=instance_create(x,y,obj_KameLeft);
            kame.direction=180;
            kame.speed=15;
            }
        }
    }  

    //Deteniendo animación de ataque  
   if keyboard_check_released(global.Disparar)
   {
