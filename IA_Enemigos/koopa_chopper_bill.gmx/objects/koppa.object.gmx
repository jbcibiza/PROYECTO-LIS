            <string>//Top
if collision_rectangle(x-20, y-100, x+20,  y-30, obj_player, false, false)
{
    motion_set(0, 0);
    if forma == 0 
    {
        sprite_index = koppa_kill_shell;
        sprite_index = koppa_shell_stop;
        with(obj_player) 
        {
            if place_empty(obj_player.x, obj_player.y+1) 
            {
                vspeed = -10;
            }
        }
        forma = 1;
    }
    else if forma == 1 
    {
        if obj_player.direction &gt; 0
        {
            motion_set(180, 2);
        }
        else
        {
            motion_set(0, 2);
        }
        with(obj_player) 
        {
            if place_empty(obj_player.x, obj_player.y+1) 
            {
                vspeed = -10;
            }
        }
        forma = 2;
    }
    else if forma == 2
    {
        instance_destroy();
    }

}
else
{
    if forma == 1
    {
        sprite_index = koppa_shell_move;
        if obj_player.direction &gt; 0
        {
            motion_set(180, 2);
        }
        else
        {
            motion_set(0, 2);
        }
        forma = 2;
    }
    else if forma == 0 or forma == 2 
    {
        with(obj_player) 
        {
            instance_destroy();
        }
    }
}


